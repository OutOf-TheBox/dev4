    <fields>
        <fullName>rmaAssetNrdIsInEffect__c</fullName>
        <description>Returns &quot;Yes&quot; if NRD is in effect.  Returns &quot;No&quot; otherwise.</description>
        <externalId>false</externalId>
        <formula>IF(OR(ISPICKVAL(rmaComponent__c, &quot;HDD&quot;),
      ISPICKVAL(rmaComponent__c, &quot;SSD&quot;)),
   IF(ISBLANK(rmaOutgoingShipmentDateTimeOpened__c),
      IF(OR(AND(ISBLANK(rmaDiskShelfAsset__c),
                NOT(ISBLANK(rmaCaseNumber__r.Asset.assetNrdEndDate__c)),
                (TODAY() &lt;= rmaCaseNumber__r.Asset.assetNrdEndDate__c)),
            AND(NOT(ISBLANK(rmaDiskShelfAsset__c)),
                NOT(ISBLANK(rmaDiskShelfAsset__r.assetNrdEndDate__c)),
                (TODAY() &lt;= rmaDiskShelfAsset__r.assetNrdEndDate__c))),
         &quot;Yes&quot;,
         &quot;No&quot;),
      IF(OR(AND(ISBLANK(rmaDiskShelfAsset__c),
                NOT(ISBLANK(rmaCaseNumber__r.Asset.assetNrdEndDate__c)),
                (DATEVALUE(rmaOutgoingShipmentDateTimeOpened__c) &lt;= rmaCaseNumber__r.Asset.assetNrdEndDate__c)),
            AND(NOT(ISBLANK(rmaDiskShelfAsset__c)),
                NOT(ISBLANK(rmaDiskShelfAsset__r.assetNrdEndDate__c)),
                (DATEVALUE(rmaOutgoingShipmentDateTimeOpened__c) &lt;= rmaDiskShelfAsset__r.assetNrdEndDate__c))),
         &quot;Yes&quot;,
         &quot;No&quot;)),
   &quot;No&quot;)</formula>
        <label>Asset NRD Is In Effect</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
