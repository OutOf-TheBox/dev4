        <formula>IF(ISBLANK(rmaDiskShelfAsset__c),
   IF(ISBLANK(rmaCaseNumber__r.Asset.assetPartShipLastValidatedDatetime__c),
      true,
      IF(TODAY() - DATEVALUE(rmaCaseNumber__r.Asset.assetPartShipLastValidatedDatetime__c) &lt;= 180,
         false,
         true
        )
     ),
   IF(ISBLANK(rmaDiskShelfAsset__r.assetPartShipLastValidatedDatetime__c),
      true,
      IF(TODAY() - DATEVALUE(rmaDiskShelfAsset__r.assetPartShipLastValidatedDatetime__c) &lt;= 180,
         false,
