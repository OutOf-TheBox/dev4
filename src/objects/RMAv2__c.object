    <fields>
        <fullName>rmaPartFailedWithinOpsTimePeriod__c</fullName>
        <description>Returns &quot;Yes&quot; if the part failed within Operations&apos; designated time period.  Returns &quot;No&quot; otherwise.</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(rmaOutgoingShipmentDateTimeOpened__c),
   IF(OR(AND(ISBLANK(rmaDiskShelfAsset__c),
             NOT(ISBLANK(rmaCaseNumber__r.Asset.Ship_Date__c)),
             (TODAY() - rmaCaseNumber__r.Asset.Ship_Date__c &lt;= 90)),
         AND(NOT(ISBLANK(rmaDiskShelfAsset__c)),
             NOT(ISBLANK(rmaDiskShelfAsset__r.Ship_Date__c)),
             (TODAY() - rmaDiskShelfAsset__r.Ship_Date__c &lt;= 90))),
      &quot;Yes&quot;,
      &quot;No&quot;),
   IF(OR(AND(ISBLANK(rmaDiskShelfAsset__c),
             NOT(ISBLANK(rmaCaseNumber__r.Asset.Ship_Date__c)),
             (DATEVALUE(rmaOutgoingShipmentDateTimeOpened__c) - rmaCaseNumber__r.Asset.Ship_Date__c &lt;= 90)),
         AND(NOT(ISBLANK(rmaDiskShelfAsset__c)),
             NOT(ISBLANK(rmaDiskShelfAsset__r.Ship_Date__c)),
             (DATEVALUE(rmaOutgoingShipmentDateTimeOpened__c) - rmaDiskShelfAsset__r.Ship_Date__c &lt;= 90))),
      &quot;Yes&quot;,
      &quot;No&quot;))</formula>
        <label>Part Failed Within Ops Time Period</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>rmaPartIsAShipOnlyPart__c</fullName>
        <description>Returns &quot;Yes&quot; if the part is a ship-only part.  Returns &quot;No&quot; otherwise.</description>
        <externalId>false</externalId>
        <formula>IF(OR(ISPICKVAL(rmaComponent__c, &quot;BEZEL&quot;),
      ISPICKVAL(rmaComponent__c, &quot;CABLE&quot;),
      ISPICKVAL(rmaComponent__c, &quot;CORD (power cord)&quot;),
      ISPICKVAL(rmaComponent__c, &quot;DONGLE&quot;),
      ISPICKVAL(rmaComponent__c, &quot;RAILS&quot;)),
   &quot;Yes&quot;,
   &quot;No&quot;)</formula>
        <label>Part Is A Ship Only Part</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
