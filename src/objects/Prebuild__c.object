    <validationRules>
        <fullName>Only_1_Prebuild_per_Opportunity</fullName>
        <active>true</active>
        <errorConditionFormula>if( Opportunity__r.Prebuild_Count_Rollup__c == 1 &amp;&amp;  ISNULL(CreatedDate) ,true,false)</errorConditionFormula>
        <errorMessage>This Opportunity already has a Prebuild record !</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Requested_Status_for_SalesReps</fullName>
        <active>true</active>
        <errorConditionFormula>if( ($Profile.Name==&apos;NS-SALES-FieldSales-Sales Rep [w/Export]&apos; || $Profile.Name==&apos;NS-SALES-FieldSales-Sales Rep [w/o Export]&apos; || $Profile.Name==&apos;NS-SALES-FieldSales-Sales Mgr [w/o Margin]&apos; || $Profile.Name==&apos;NS-SALES-FieldSales-Sales Mgr [w/Margin]&apos;) &amp;&amp; !ISPICKVAL(Prebuild_Status__c,&apos;Requested&apos;),true,false)</errorConditionFormula>
        <errorDisplayField>Prebuild_Status__c</errorDisplayField>
        <errorMessage>Sales Reps can only choose Prebuild Status as &apos;Requested&apos; !</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SalesReps_NO_changes_except_Rejected</fullName>
        <active>true</active>
        <errorConditionFormula>if(( ($Profile.Name==&apos;NS-SALES-FieldSales-Sales Rep [w/Export]&apos; || $Profile.Name==&apos;NS-SALES-FieldSales-Sales Rep [w/o Export]&apos; || $Profile.Name==&apos;NS-SALES-FieldSales-Sales Mgr [w/o Margin]&apos; || $Profile.Name==&apos;NS-SALES-FieldSales-Sales Mgr [w/Margin]&apos;) &amp;&amp;   NOT(ISNULL(CreatedDate))   &amp;&amp; 
 NOT(ISPICKVAL(PRIORVALUE( Prebuild_Status__c ),&apos;Rejected&apos;))),true,false )</errorConditionFormula>
        <errorMessage>SalesReps cannot make any changes once record has been saved !</errorMessage>
    </validationRules>
