    <validationRules>
        <fullName>Do_not_Allow_selecting_different_gift</fullName>
        <active>false</active>
        <description>Do not Allow_selecting_different_gift unless checkbox set.</description>
        <errorConditionFormula>AND(!ISBLANK(TEXT(Gift__c)),
TEXT(Preferred_Gift__c) &lt;&gt; TEXT(Gift__c),
!Allow_selecting_different_gift__c)</errorConditionFormula>
        <errorDisplayField>Allow_selecting_different_gift__c</errorDisplayField>
        <errorMessage>The Gift selected is different from the Preferred Gift. If you would like to proceed with the different gift, check the &quot;Allow_selecting_different_gift&quot; box.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Do_not_allow_Preferred_Gift</fullName>
        <active>true</active>
        <errorConditionFormula>AND(!ISPICKVAL(Preferred_Gift__c,&quot;&quot;),
 SPIFF_Program_Name__c &lt;&gt; &quot;Q3 FY15 Appointment Setting&quot;, SPIFF_Program_Name__c &lt;&gt; &quot;Q2 FY16 Appointment Setting&quot;
)</errorConditionFormula>
        <errorDisplayField>Preferred_Gift__c</errorDisplayField>
        <errorMessage>Preferred Gift is not an option for this SPIFF Program.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Force_Conscious_Override_of_Evaluation_R</fullName>
        <active>true</active>
        <description>Before Approving a Claim with Evaluation Result not &quot;Approve&quot;, you must set the &quot;Override Evaluation Result&quot; checkbox.</description>
        <errorConditionFormula>AND(  
     Override_Evaluation_Result__c = FALSE,
    Evaluation_Result__c != &quot;Approve&quot;,
    ISPICKVAL(Claim_Status__c, &quot;Approved&quot;)  
)</errorConditionFormula>
        <errorDisplayField>Override_Evaluation_Result__c</errorDisplayField>
        <errorMessage>Before Approving a Claim with Evaluation Result not being &quot;Approve&quot;, you must set the &quot;Override Evaluation Result&quot; checkbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Force_Gift_Card_Number</fullName>
        <active>true</active>
        <description>Before Approving, for Gifts Delivered by Email, you must set the Gift Card Number.</description>
        <errorConditionFormula>AND(  
ISPICKVAL(  Gift_Delivery__c ,&quot;Email&quot;),
ISBLANK(Gift_Card_Number__c  ),
ISPICKVAL(Claim_Status__c, &quot;Approved&quot;)  
)</errorConditionFormula>
        <errorDisplayField>Gift_Card_Number__c</errorDisplayField>
        <errorMessage>Before Approving, for Gifts Delivered by Email, you must set the Gift Card Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Force_Gift_Value</fullName>
        <active>false</active>
        <errorConditionFormula>AND( ISBLANK( Gift_Value__c ), ISPICKVAL( Gift_Delivery__c , &quot;Tangible&quot;)  )</errorConditionFormula>
        <errorDisplayField>Gift_Value__c</errorDisplayField>
        <errorMessage>For Tangible gifts, you must manually set the Gift Value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Force_Gift_information</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( Gift__c, &quot;&quot; ), 
ISPICKVAL(Claim_Status__c, &quot;Approved&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Gift__c</errorDisplayField>
        <errorMessage>Before Approving, you must set the Gift information.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Force_No_Gift_Card_Number</fullName>
        <active>true</active>
        <description>Gift Card Number should not be set for Tangible / Via Reseller Payroll Gifts.</description>
        <errorConditionFormula>AND(  
OR(ISPICKVAL( Gift_Delivery__c  ,&quot;Tangible&quot;),ISPICKVAL( Gift_Delivery__c  ,&quot;Via Reseller Payroll&quot;)),
Gift_Card_Number__c &lt;&gt; &quot;&quot;
)</errorConditionFormula>
        <errorDisplayField>Gift_Card_Number__c</errorDisplayField>
        <errorMessage>Gift Card Number should not be set for Tangible or Via Reseller Payroll Gifts.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Force_Override_Evaluation_Result_Reason</fullName>
        <active>true</active>
        <description>For Override_Evaluation_Result = TRUE, Please specify the Reason for overriding the Evaluation Result.</description>
        <errorConditionFormula>AND(
    Override_Evaluation_Result__c = TRUE,
    ISBLANK( Override_Evaluation_Result_Reason__c )
)</errorConditionFormula>
        <errorDisplayField>Override_Evaluation_Result_Reason__c</errorDisplayField>
        <errorMessage>Please specify the Reason for overriding the Evaluation Result.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Claim_Status_to_Disqualified_if_Dup</fullName>
        <active>true</active>
        <description>Do not allow setting of Claim Status to Disqualified if Claim is Duplicate.</description>
        <errorConditionFormula>ISCHANGED( Claim_Status__c )  &amp;&amp; ( ISPICKVAL( Claim_Status__c , &quot;Disqualified&quot;) &amp;&amp;  Evaluation_Result_old__c  = &quot;Review - Duplicate&quot; )</errorConditionFormula>
        <errorDisplayField>Claim_Status__c</errorDisplayField>
        <errorMessage>You may not set the Claim Status to &quot;Disqualified&quot; if the Claim is a Duplicate.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Clearing_Override_Result_if_Gift_Sent</fullName>
        <active>true</active>
        <description>Do not allow clearing the Override Evaluation Result checkbox, if Claim Status = Gift Sent.</description>
        <errorConditionFormula>AND(
Override_Evaluation_Result__c = FALSE,
ISPICKVAL( Claim_Status__c , &quot;Gift Sent&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Override_Evaluation_Result__c</errorDisplayField>
        <errorMessage>You may not clear the Override Evaluation Result checkbox, if Claim Status = Gift Sent.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Manual_Set_Claim_Status_Dupl_Notified</fullName>
        <active>true</active>
        <description>Do not allow manual setting of Claim Status to Duplicate (Notified)</description>
        <errorConditionFormula>ISCHANGED( Claim_Status__c )  &amp;&amp; ( ISPICKVAL( Claim_Status__c , &quot;Duplicate (Notified)&quot;))</errorConditionFormula>
        <errorDisplayField>Claim_Status__c</errorDisplayField>
        <errorMessage>You may not set claim status to &quot;Duplicate (Notified)&quot;. Select &quot;Duplicate&quot; instead.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Manual_Update</fullName>
        <active>true</active>
        <description>Do not allow Manual Update per Claim Status.</description>
        <errorConditionFormula>AND(
Manual_Update__c = TRUE,
ISPICKVAL( Claim_Status__c, &quot;Gift Sent&quot; )
)</errorConditionFormula>
        <errorDisplayField>Manual_Update__c</errorDisplayField>
        <errorMessage>You may not Manual Update if Claim status is Gift Sent.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Manual_set_Claim_Status_to_Gift_Sent</fullName>
        <active>false</active>
        <description>Do not allow manual setting of Claim Status to Gift Sent.</description>
        <errorConditionFormula>ISCHANGED( Claim_Status__c )  &amp;&amp; ( ISPICKVAL( Claim_Status__c , &quot;Gift Sent&quot;)  )</errorConditionFormula>
        <errorDisplayField>Claim_Status__c</errorDisplayField>
        <errorMessage>You may not manually set the Claim Status to &quot;Gift Sent&quot;. Set to &quot;Approved&quot; instead.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>UpdateSPIFF_Claim</fullName>
        <availability>online</availability>
        <description>Manually force the Apex functionality to update SPIFF Claim</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Update SPIFF Claim</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)}
if({!SPIFF_Claim__c.Manual_Update__c} == 1){
var label = this.value;
this.value = &quot;Updating SPIFF Claim&quot;;
this.disabled = true;
this.className = &quot;btnDisabled&quot;;
var rs;
try{
setTimeout(function(){
var claimId = &apos;{!SPIFF_Claim__c.Id}&apos;;
rs = sforce.apex.execute(&quot;SPIFFClaim&quot;, &quot;manualUpdateSPIFF_Claim&quot;, {&quot;claimId&quot;:claimId});
if(rs!=&apos;&apos;) alert(rs);
window.location = window.location.href;
},500);
}catch(err){
this.value = label;
this.disabled = false;
this.className = &quot;btn&quot;;
alert(err);
}
}else{
alert(&apos;For the Update SPIFF Claim button to work, you must first enter Manual Update mode&apos;);
}</url>
    </webLinks>
</CustomObject>
