<apex:page docType="html-5.0" sidebar="false" showHeader="false" standardStylesheets="false" cache="false" language="{!$CurrentPage.parameters.l}" >
<html lang="en">
 <head>
  <title>Mobile Customer Service</title>
	<meta name="viewport" content="initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0"/>
	<meta name="apple-mobile-web-app-capable" content="yes" />
	<meta name="apple-mobile-web-app-status-bar-style" content="black" />
	<apex:stylesheet value="{!URLFOR($Resource.pkb_mobile,'css/normalize.css')}" />
	<link rel="apple-touch-icon" href="{!URLFOR($Resource.pkb_mobile, 'imgs/appIcon.png')}" />
	<c:pkb_mobile_css />
	<c:pkb_mobile_templates />
	<c:pkb_mobile_jsInit />
	<script src="{!URLFOR($Resource.pkb_mobile, 'js/libs/iscroll/iscroll.js')}"></script>
	<script
		data-main="{!URLFOR($Resource.pkb_mobile, 'js/main')}"
